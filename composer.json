{
    "name": "ogogo/ogogo-zfc-user-pm",
    "description": "Private message system for ZfcUser",
    "license": "MIT",
    "keywords": [
        "zfc-user",
        "zendframework",
        "module",
        "ogogo",
        "pm",
        "private message"
    ],
    "homepage": "https://github.com/Ogogo/OgogoZfcUserPm",
    "authors": [
        {
            "name": "Finbarr McCarthy",
            "email": "finbarr@finbarr.org",
            "homepage": "http://www.ogogo.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    },
    "require": {
        "php": "~5.4",
        "zf-commons/zfc-user": "*",
        "ogogo/ogogo-base": "~0.0.1",
        "rhumsaa/uuid": "2.*",
        "zendframework/zend-serializer": "~2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0.0",
        "squizlabs/php_codesniffer": "~2.0.0@DEV",
        "zf-commons/zfc-user-doctrine-orm": "~1.0",
        "codeclimate/php-test-reporter": "0.*"
    },
    "autoload": {
        "psr-0": {
            "Ogogo\\ZfcUser\\Pm\\": "src/"
        }
    },
    "suggest": {
        "zf-commons/zfc-user-doctrine-orm": "ZF2 Doctrine ORM module for use with the Doctrine ORM mapper"
    }
}
